          <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="jira.lastLoginAssertion" enabled="true">
            <stringProp name="BeanShellAssertion.query">import java.io.*;
import javax.xml.parsers.*;
import javax.xml.xpath.*;
import org.w3c.dom.*;
import org.w3c.tidy.Tidy;
import java.util.Arrays;
import org.joda.time.DateTime;
import org.joda.time.Period;
import org.joda.time.Days;
import org.joda.time.Months;
import java.text.SimpleDateFormat;
import java.text.ParseException;
import java.util.Date;
import java.util.Locale;

String aDate = vars.get(&quot;lastLogin&quot;);
String userName = vars.get(&quot;user&quot;);



int difference = -1;
if((aDate.equals(&quot;Never logged in&quot;))||(aDate.equals(&quot;Not recorded&quot;))||(aDate.equals(&quot;Not Found&quot;))) {
	difference = -1;
} else if( aDate.startsWith(&quot;Today&quot;) || aDate.startsWith(&quot;Yesterday&quot;) || aDate.startsWith(&quot;Monday&quot;) || aDate.startsWith(&quot;Tuesday&quot;) || aDate.startsWith(&quot;Wednesday&quot;) || aDate.startsWith(&quot;Thursday&quot;) || aDate.startsWith(&quot;Friday&quot;) || aDate.startsWith(&quot;Saturday&quot;) || aDate.startsWith(&quot;Sunday&quot;) ) {
	difference = 0;
} else {
	try {
     	SimpleDateFormat formatter = new SimpleDateFormat(&quot;dd/MMM/yy hh:mm a&quot;, Locale.ENGLISH);
          Date date = formatter.parse(aDate);
          DateTime start = new DateTime(date);
          DateTime now = new DateTime();

          Period period = new Period(start, now);

          Days days = Days.daysBetween(start, now);
          difference = days.getDays();
     } catch(ParseException e) {
     	log.info(&quot;cannot parse aDate: \&quot;&quot; + aDate + &quot;\&quot;&quot;);
          log.info(&quot;stacktrace: &quot; + e.getMessage());
     }

}

log.info(userName +&quot;,&quot;+aDate+&quot;,&quot;+difference);

if(difference == -1) {
	Failure = true;
	FailureMessage = userName + &quot;: Last login date not found.\t&quot;;
} else if(difference &lt; 90) {

	Failure = true;
	FailureMessage = &quot;[&quot;+userName+&quot;] jira: Last login &gt; 3 months (&quot; + difference + &quot; days).\n\t&quot;;
}</stringProp>
            <stringProp name="BeanShellAssertion.filename"></stringProp>
